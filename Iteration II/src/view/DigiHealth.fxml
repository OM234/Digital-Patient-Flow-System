<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="503.0" prefWidth="755.0"
            xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.DigiHealthController">
    <children>
        <Label fx:id="DigiHealthLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="46.0" prefWidth="600.0"
               text="DigiHealth" textFill="WHITE" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
               AnchorPane.topAnchor="0.0">
            <font>
                <Font name="System Bold" size="16.0"/>
            </font>
        </Label>
        <Button fx:id="viewUnitsButton" layoutX="14.0" layoutY="132.0" mnemonicParsing="false"
                onAction="#TableViewAppear" prefHeight="54.0" prefWidth="116.0" text="All Units"
                AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="132.0"/>
        <Button fx:id="viewPatientsButton" layoutX="14.0" layoutY="197.0" mnemonicParsing="false"
                onAction="#TableViewAppear" prefHeight="54.0" prefWidth="116.0" text="All Patients" textOverrun="CLIP"
                AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="197.0"/>
        <TableView fx:id="unitsTableView" layoutX="155.0" layoutY="132.0" prefHeight="333.0" prefWidth="445.0"
                   visible="false" AnchorPane.bottomAnchor="38.0" AnchorPane.leftAnchor="155.0"
                   AnchorPane.rightAnchor="155.0" AnchorPane.topAnchor="132.0"/>
        <TableView fx:id="patientsTableView" layoutX="155.0" layoutY="132.0" prefHeight="333.0" prefWidth="445.0"
                   visible="false" AnchorPane.bottomAnchor="38.0" AnchorPane.leftAnchor="155.0"
                   AnchorPane.rightAnchor="155.0" AnchorPane.topAnchor="132.0"/>
        <Label fx:id="bottomViewingLabel" alignment="CENTER" layoutX="164.0" layoutY="368.0" prefHeight="24.0"
               prefWidth="417.0" text="Label" visible="false" AnchorPane.bottomAnchor="6.0"
               AnchorPane.leftAnchor="164.0" AnchorPane.rightAnchor="173.39999999999998"/>
        <Button fx:id="patientsOnUnitButton" alignment="BASELINE_CENTER" contentDisplay="CENTER" disable="true"
                layoutX="625.0" layoutY="132.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                minWidth="-Infinity" mnemonicParsing="false" onAction="#populatePatientsOnUnitTable" opacity="0.3"
                prefHeight="54.0" prefWidth="116.0" text="Patients On &#10;Unit&#10;" textAlignment="CENTER"
                textOverrun="CLIP" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="132.0">
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
        </Button>
        <GridPane alignment="CENTER" layoutX="276.0" layoutY="64.0" prefHeight="54.0" prefWidth="203.0"
                  AnchorPane.leftAnchor="276.0" AnchorPane.rightAnchor="276.0" AnchorPane.topAnchor="64.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Button fx:id="addButton" disable="true" maxHeight="-Infinity" maxWidth="-Infinity"
                        minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#addUnitOrPatient"
                        prefHeight="46.0" prefWidth="59.0" textAlignment="CENTER" GridPane.halignment="RIGHT"
                        GridPane.hgrow="NEVER" GridPane.valignment="CENTER" GridPane.vgrow="NEVER">
                    <graphic>
                        <ImageView fitHeight="36.0" fitWidth="46.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@Images/Plus%20sign.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="removeButton" disable="true" maxHeight="-Infinity" maxWidth="-Infinity"
                        minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#deletePatOrUnit"
                        prefHeight="46.0" prefWidth="59.0" textAlignment="CENTER" GridPane.columnIndex="1"
                        GridPane.halignment="LEFT" GridPane.hgrow="NEVER" GridPane.valignment="CENTER"
                        GridPane.vgrow="NEVER">
                    <graphic>
                        <ImageView fitHeight="12.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@Images/minus%20sign.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
            </children>
        </GridPane>
    </children>
</AnchorPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane focusTraversable="true" prefHeight="185.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.passPromptController">
   <children>
      <Label alignment="CENTER" prefHeight="48.0" prefWidth="400.0" text="Welcome to DigiHealth" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Label fx:id="userIDButton" alignment="CENTER" layoutY="57.0" prefHeight="36.0" prefWidth="154.0" text="User ID" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="57.0" />
      <Label fx:id="passButton" alignment="CENTER" layoutY="93.0" prefHeight="36.0" prefWidth="154.0" text="Password" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="93.0" />
      <TextField fx:id="userIDTextBox" alignment="CENTER" layoutX="154.0" layoutY="57.0" onMouseClicked="#turnUserBoxToDefaultColor" prefHeight="36.0" prefWidth="185.0" promptText="Enter your user ID" AnchorPane.leftAnchor="154.0" AnchorPane.topAnchor="57.0" />
      <PasswordField fx:id="passPassField" alignment="CENTER" layoutX="154.0" layoutY="93.0" prefHeight="36.0" prefWidth="185.0" promptText="Enter your password" AnchorPane.leftAnchor="154.0" AnchorPane.topAnchor="93.0" />
      <Button fx:id="enterButton" layoutY="142.0" mnemonicParsing="false" onAction="#passChecker" prefHeight="42.0" prefWidth="400.0" text="Enter" AnchorPane.bottomAnchor="-1.0" AnchorPane.leftAnchor="-1.0" AnchorPane.rightAnchor="-1.0">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
